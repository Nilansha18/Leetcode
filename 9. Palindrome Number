bool isPalindrome(int x) {
        string str=to_string(x);

        for(int i=0 , j=str.length()-1; i<str.length()/2 && j>=str.length()/2; i++ , j--){
            cout<<str[i]<<" "<<str[j]<<endl;
            if(str[i]==str[j]) continue;
            else return false;
        }
        return true;
        
    }
